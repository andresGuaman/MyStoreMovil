<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".fracments.menu_barras">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="565dp"
        android:orientation="vertical">

        <Button
            android:id="@+id/button"
            android:layout_width="match_parent"
            android:layout_height="80dp"
            android:background="#FFFDFD"
            android:text="Mis pedidos"
            android:textAllCaps="false"
            android:textSize="20dp" />

        <Button
            android:id="@+id/button3"
            android:layout_width="match_parent"
            android:layout_height="80dp"
            android:background="#FFFDFD"
            android:text="Mi perfil"
            android:textAllCaps="false"
            android:textSize="20dp" />

        <Button
            android:id="@+id/button6"
            android:layout_width="match_parent"
            android:layout_height="80dp"
            android:background="#FFFDFD"
            android:text="Informacion de la Tienda"
            android:textAllCaps="false"
            android:textSize="20dp" />

        <Button
            android:id="@+id/button8"
            android:layout_width="match_parent"
            android:layout_height="80dp"
            android:background="#FFFDFD"
            android:text="Servicio al Cliente"
            android:textAllCaps="false"
            android:textSize="20dp" />

        <Button
            android:id="@+id/button9"
            android:layout_width="match_parent"
            android:layout_height="80dp"
            android:background="#FFFDFD"
            android:text="¿Qué es MyStore?"
            android:textAllCaps="false"
            android:textSize="20dp" />

        <Button
            android:id="@+id/btn_init_secion"
            android:layout_width="match_parent"
            android:layout_height="80dp"
            android:background="#FFFDFD"
            android:text="Iniciar sesión"
            android:textAllCaps="false"
            android:textSize="20dp" />

        <Button
            android:id="@+id/btn_admin"
            android:layout_width="match_parent"
            android:layout_height="80dp"
            android:background="#FFFDFD"
            android:text="Administración [Empleado]"
            android:textAllCaps="false"
            android:textSize="20dp" />

    </LinearLayout>
</FrameLayout>